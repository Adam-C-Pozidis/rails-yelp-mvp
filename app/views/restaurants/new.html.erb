<h1> New Restaurant</h1>
<%= simple_form_for [@restaurant] do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :category %>
  <%= f.input :phone_number %>
  <%= f.submit "Submit", class: "btn btn-primary" %>
<% end %>
<p><%= link_to "back to all", restaurants_path, class: "btn btn-primary"%></p>
